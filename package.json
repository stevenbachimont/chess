{
  "name": "chess-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "frontend": "cd frontend && npm run dev",
    "backend": "cd backend && npm run start",
    "dev": "concurrently \"npm run frontend\" \"npm run backend\"",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  }
}
